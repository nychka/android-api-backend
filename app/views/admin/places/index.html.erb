<h1>Listing admin_places</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Lng</th>
      <th>Lat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_places.each do |admin_place| %>
      <tr>
        <td><%= admin_place.name %></td>
        <td><%= admin_place.phone %></td>
        <td><%= admin_place.lng %></td>
        <td><%= admin_place.lat %></td>
        <td><%= link_to 'Show', admin_place %></td>
        <td><%= link_to 'Edit', edit_admin_place_path(admin_place) %></td>
        <td><%= link_to 'Destroy', admin_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place', new_admin_place_path %>
