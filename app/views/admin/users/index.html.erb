<!-- show all ads-->
<% unless @users.empty? %>
  <% @users.each do |user| %>
  <%= render partial: 'user', locals: { user: user } %>
  <% end %>
<% else %>
  <!-- no ads found -->
<% end %>
<div class='clearfix'></div>

<%= render 'shared/pagination', items: @users %>
<%= render 'shared/create_item', path: new_admin_user_path %>
